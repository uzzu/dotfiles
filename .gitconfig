[user]
    name = uzzu
    email = himeatball@gmail.com
[github]
    user = uzzu
[core]
    editor = vim
[merge]
    tool = vimdiff
[color]
    ui = true
[alias]
    br      = branch
    bra     = branch    -a
    brd     = branch    -d
    brdd    = branch    -D
    co      = checkout
    cob     = checkout  -b
    ci      = commit
    cm      = commit    -m
    cv      = commit    -v
    cam     = commit    -am
    cav     = commit    -av
    dd      = diff
    ds      = diff      --cached
    di      = diff      HEAD~
    dii     = diff      HEAD~~
    diii    = diff      HEAD~~~
    diiii   = diff      HEAD~~~~
    diiiii  = diff      HEAD~~~~~
    fe      = fetch
    fep     = fetch     --prune
    info    = remote    show origin
    mnff    = merge     --no-ff
    pr      = pull      --rebase
    st      = status    -sb
    sti     = status    -sb --ignored
    tr      = log               --graph --date=local --pretty='format:%C(green)%h%Creset %cd %C(cyan)%an%Creset - %s %C(red)%d%Creset'
    tra     = log       --all   --graph --date=local --pretty='format:%C(green)%h%Creset %cd %C(cyan)%an%Creset - %s %C(red)%d%Creset'
    undo    = reset     --soft HEAD^

